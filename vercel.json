{
  "version": 2,
  "cleanUrls": true,
  "routes": [
    { "src": "^/admin/?$", "dest": "/admin/index.html" },
    { "src": "^/admin/contracts/?$", "dest": "/admin/contracts/index.html" },
    { "src": "^/admin/musicians/?$", "dest": "/admin/musicians/index.html" },
    { "src": "^/admin/ti-machann/?$", "dest": "/admin/ti-machann/index.html" },
    { "src": "^/admin/transactions/?$", "dest": "/admin/transactions/index.html" }
  ]
}{
  "version": 2,
  "cleanUrls": true,
  "routes": [
    { "src": "^/admin/?$", "dest": "/admin/index.html" }
  ],
  "redirects": [
    { "source": "http://briyantsoleysigno.com/:path*", "destination": "https://briyantsoleysigno.com/:path*", "permanent": true }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" },
        { "key": "Permissions-Policy", "value": "geolocation=(), microphone=(), camera=()" }
      ]
    }
  ]
}{
  "version": 2,
  "trailingSlash": false,
  "cleanUrls": true,
  "routes": [
    { "src": "/admin/?", "dest": "/admin/index.html" }
  ]
}{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/admin", "destination": "/admin/index.html" },
    { "source": "/admin/ti-machann", "destination": "/admin/ti-machann.html" }
  ]
}
